<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="625.0" prefWidth="867.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="diagrams.pViz.idmap.BridgeDbController">
  <children>
      <HBox VBox.vgrow="NEVER">
         <children>
          <MenuBar HBox.hgrow="ALWAYS">
            <menus>
              <Menu mnemonicParsing="false" text="File">
                <items>
                  <MenuItem mnemonicParsing="false" text="New" />
                  <MenuItem mnemonicParsing="false" text="Open…" />
                  <Menu mnemonicParsing="false" text="Open Recent" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Close" />
                  <MenuItem mnemonicParsing="false" text="Save" />
                  <MenuItem mnemonicParsing="false" text="Save As…" />
                  <MenuItem mnemonicParsing="false" text="Revert" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Preferences…" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Quit" />
                </items>
              </Menu>
              <Menu mnemonicParsing="false" text="Edit">
                <items>
                  <MenuItem mnemonicParsing="false" text="Undo" />
                  <MenuItem mnemonicParsing="false" text="Redo" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Cut" />
                  <MenuItem mnemonicParsing="false" text="Copy" />
                  <MenuItem mnemonicParsing="false" text="Paste" />
                  <MenuItem mnemonicParsing="false" onAction="#deleteSelectedRows" text="Delete" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Select All" />
                  <MenuItem mnemonicParsing="false" text="Unselect All" />
                </items>
              </Menu>
              <Menu mnemonicParsing="false" text="Help">
                <items>
                  <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
                </items>
              </Menu>
            </menus>
          </MenuBar>
            <Label text="Species" />
            <ChoiceBox fx:id="organism" />
            <Button fx:id="search" mnemonicParsing="false" onAction="#doSearch" text="Search" />
            <Button fx:id="addRowBtn" layoutX="816.0" layoutY="10.0" mnemonicParsing="false" onAction="#addRow" text="Add" />
            <Button fx:id="sources" layoutX="773.0" layoutY="10.0" mnemonicParsing="false" onAction="#doSourceLinkout" text="Sources" />
         </children>
      </HBox>
      <SplitPane dividerPositions="0.4" VBox.vgrow="ALWAYS">
         <items>
            <SplitPane dividerPositions="0.3, 0.6" orientation="VERTICAL">
               <items>
                  <VBox>
                     <children>
                        <Label contentDisplay="RIGHT" minWidth="100.0" text="Targets" VBox.vgrow="NEVER">
                           <VBox.margin>
                              <Insets left="2.0" />
                           </VBox.margin>
                        </Label>
                        <ListView fx:id="targetList" prefHeight="577.0" prefWidth="147.0" VBox.vgrow="ALWAYS" />
                     </children>
                  </VBox>
                  <VBox layoutX="10.0" layoutY="10.0">
                     <children>
                        <HBox VBox.vgrow="NEVER">
                           <VBox.margin>
                              <Insets left="2.0" />
                           </VBox.margin>
                           <children>
                              <Label contentDisplay="RIGHT" minWidth="100.0" text="Pathways" />
                              <TextField fx:id="searchPathwaysBox" />
                              <Button fx:id="searchPathways" mnemonicParsing="false" onAction="#doSearchPathways" text="Search" />
                           </children>
                        </HBox>
                        <TableView fx:id="pathwayTable" VBox.vgrow="ALWAYS">
                          <columns>
                            <TableColumn fx:id="idColumn" prefWidth="60.0" text="ID" />
                            <TableColumn fx:id="urlColumn" prefWidth="56.0" text="URL" />
                              <TableColumn fx:id="nameColumn" prefWidth="88.0" text="Name" />
                              <TableColumn fx:id="speciesColumn" prefWidth="201.0" text="Species" />
                              <TableColumn fx:id="revisionColumn" prefWidth="110.0" text="Revision" />
                          </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                     </children>
                  </VBox>
                  <VBox layoutX="10.0" layoutY="10.0">
                     <children>
                        <Label contentDisplay="RIGHT" minWidth="100.0" text="Filters" VBox.vgrow="NEVER">
                           <VBox.margin>
                              <Insets left="2.0" />
                           </VBox.margin>
                        </Label>
                        <ListView fx:id="filterList" prefHeight="577.0" prefWidth="147.0" VBox.vgrow="ALWAYS" />
                     </children>
                  </VBox>
               </items>
            </SplitPane>
            <VBox>
               <children>
                  <Label text="Results" />
                  <TableView fx:id="resultsTable" editable="true" VBox.vgrow="ALWAYS">
                     <opaqueInsets>
                        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                     </opaqueInsets>
                  </TableView>
               </children>
            </VBox>
         </items>
      </SplitPane>
  </children>
</VBox>
